Algoritmo Promedio 
	Definir Estudiante Como caracter;
	Definir num1, num2, num3 Como Entero;
	Definir definitiva como entero; 
	Escribir "digite el nombre del Alumno";
	Leer Estudiante;
	Escribir "Digite el num1";
	Leer num1;
	Escribir "Digite el num2";
	Leer num2;
	Escribir "Digite el num3";
	Leer num3;
	definitiva<-(num1+num2+num3)/3
	Escribir "resultado del promedio " definitiva;
FinAlgoritmo
